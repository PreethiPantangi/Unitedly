<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context="com.example.admin.unitedly.ContactFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/textView"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginTop="48dp"
        android:layout_gravity="center_horizontal"
        android:fontFamily="sans-serif"
        android:text="Stay connected"
        android:textColor="#363636"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_marginTop="100dp"
        android:lineSpacingExtra="8sp"
        android:text="Plot No. 8-5/4, Bachupally, Nizampet Road, Rajiv Gandhi Nagar Colony, Hyderabad, Telangana - 500090"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="18sp"
        android:textColor="@android:color/black" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_marginTop="200dp"
        android:lineSpacingExtra="8sp"
        android:text="Questions? Write a mail to us at - team.unitedly@gmail.com "
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="18sp"
        android:textColor="@android:color/black"
        android:visibility="visible" />

</FrameLayout>
